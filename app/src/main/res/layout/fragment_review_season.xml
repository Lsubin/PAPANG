<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   tools:context="com.example.perfume.review.ReviewSeasonFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/s_frame1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent">

        <TextView
                android:id="@+id/s_text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="봄"
                android:fontFamily="@font/kopubworlddotumedium"
                android:textSize="10sp"
                android:textColor="#4B4B4B"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"/>

        <TextView
                android:id="@+id/spring_gage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="8dp"
                android:text="100%"
                android:fontFamily="@font/kopubworlddotumedium"
                android:textSize="10sp"
                android:textColor="#4B4B4B"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintRight_toLeftOf="@id/spring_progressbar"/>

        <ProgressBar
                android:id="@+id/spring_progressbar"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="204dp"
                android:layout_height="6dp"
                android:layout_centerInParent="true"
                android:indeterminate="false"
                android:max="100"
                android:progress="100"
                android:progressDrawable="@drawable/gradient_progressbar"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/s_frame2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@id/s_frame1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent">

        <TextView
                android:id="@+id/s_text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="여름"
                android:fontFamily="@font/kopubworlddotumedium"
                android:textSize="10sp"
                android:textColor="#4B4B4B"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"/>

        <TextView
                android:id="@+id/summer_gage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="8dp"
                android:text="100%"
                android:fontFamily="@font/kopubworlddotumedium"
                android:textSize="10sp"
                android:textColor="#4B4B4B"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintRight_toLeftOf="@id/summer_progressbar"/>

        <ProgressBar
                android:id="@+id/summer_progressbar"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="204dp"
                android:layout_height="6dp"
                android:layout_centerInParent="true"
                android:indeterminate="false"
                android:max="100"
                android:progress="100"
                android:progressDrawable="@drawable/gradient_progressbar"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/s_frame3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@id/s_frame2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent">

        <TextView
                android:id="@+id/s_text3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="가을"
                android:fontFamily="@font/kopubworlddotumedium"
                android:textSize="10sp"
                android:textColor="#4B4B4B"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"/>

        <TextView
                android:id="@+id/fall_gage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="8dp"
                android:text="100%"
                android:fontFamily="@font/kopubworlddotumedium"
                android:textSize="10sp"
                android:textColor="#4B4B4B"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintRight_toLeftOf="@id/fall_progressbar"/>

        <ProgressBar
                android:id="@+id/fall_progressbar"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="204dp"
                android:layout_height="6dp"
                android:layout_centerInParent="true"
                android:indeterminate="false"
                android:max="100"
                android:progress="100"
                android:progressDrawable="@drawable/gradient_progressbar"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/s_frame4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@id/s_frame3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent">

        <TextView
                android:id="@+id/s_text4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="겨울"
                android:fontFamily="@font/kopubworlddotumedium"
                android:textSize="10sp"
                android:textColor="#4B4B4B"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"/>

        <TextView
                android:id="@+id/winter_gage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="8dp"
                android:text="100%"
                android:fontFamily="@font/kopubworlddotumedium"
                android:textSize="10sp"
                android:textColor="#4B4B4B"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintRight_toLeftOf="@id/winter_progressbar"/>

        <ProgressBar
                android:id="@+id/winter_progressbar"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="204dp"
                android:layout_height="6dp"
                android:layout_centerInParent="true"
                android:indeterminate="false"
                android:max="100"
                android:progress="100"
                android:progressDrawable="@drawable/gradient_progressbar"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>